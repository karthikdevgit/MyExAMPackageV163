    <webLinks>
        <fullName>Assessment_Form_Link</fullName>
        <availability>online</availability>
        <description>Link to Assessment form</description>
        <displayType>link</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Assessment Form Link Not</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var invest=&quot;{!IssueRequest_Life_Cycle__c.Investigation_Type__c}&quot;;
var issueids=&quot;{!IssueRequest_Life_Cycle__c.Id}&quot;;
var accountname=&quot;{!IssueRequest_Life_Cycle__c.Facility_Name__c}&quot;;
var accountids=&quot;{!IssueRequest_Life_Cycle__c.Facility_NameId__c}&quot;;
var templatenames=&quot;School Property and Safety Analysis&quot;;
var urls=&quot;/&quot;+issueids;

if (invest==&apos;School Property and Safety Analysis&apos;) {
urls=&quot;/apex/ExAM__QuestionnaireCustomIssue?issueid=&quot;+issueids+&quot;&amp;accid=&quot;+accountids+&quot;&amp;templatename=&quot;+templatenames;alert(urls);}
else if (invest==&apos;Safety and Health Inspection Checklist&apos;) {
templatenames=&quot;Safety and Health Inspection Checklist&quot;;
urls=&quot;/apex/ExAM__QuestionnaireCustomIssue?issueid=&quot;+issueids+&quot;&amp;accid=&quot;+accountids+&quot;&amp;templatename=&quot;+templatenames;alert(urls);
}
else if (invest==&apos;Multi Assessment Manager&apos;) {
urls=&quot;/apex/ExAM__MultiAssessmentManager?id=&quot;+issueids+&quot;&amp;accid=&quot;+accountids+&quot;&amp;accname=&quot;+accountname;alert(urls);
}
window.location =urls;</url>
